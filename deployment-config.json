{
  "Service": {
    "Subpath": "/services/service-name/path",
    "Build_Command": "npm run build:prod:ci"
  },
  "Development": {
    "Environment": {
      "Type": "static-ec2",
      "Routing" : "internal",
      "Suffix": "/dev/"
    },
    "Infrastructure": {
      "EC2_Type": "c5a.large"
    },
    "Build": {
      "SDK_Path": "sdk-configs/Development.json",
      "sdkContentServerUrl": "https://internal.dxapi-nonprod.ns2n.corp.hmrc.gov.uk/services/service-name/path/dev/"
    }
  },
  "Preproduction": {
    "Environment": {
      "Type": "static-ec2",
      "Routing" : "internal",
      "Suffix": "/preprod/"
    },
    "Infrastructure": {
      "EC2_Type": "c5a.large"
    },
    "Build": {
      "SDK_Path": "sdk-configs/Preproduction.json",
      "sdkContentServerUrl": "https://internal.dxapi-prod.ns2p.corp.hmrc.gov.uk/services/service-name/path/preprod/"
    }
  },
  "Staging": {
    "Environment": {
      "Type": "hpha-ec2",
      "Routing" : "internal",
      "Suffix": "/staging/"
    },
    "Infrastructure": {
      "EC2_Type": "c5a.large",
      "EC2_Minimum_Hosts" : "1",
      "EC2_Maximum_Hosts" : "2"
    },
    "Build": {
      "SDK_Path": "sdk-configs/Preproduction.json",
      "sdkContentServerUrl": "https://internal.dxapi-prod.ns2p.corp.hmrc.gov.uk/services/service-name/path/preprod/"
    }
  },
  "Production": {
    "Environment": {
      "Type": "hpha-ec2",
      "Routing" : "internal",
      "Suffix": "/"
    },
    "Infrastructure": {
      "EC2_Type": "c5a.large",
      "EC2_Minimum_Hosts" : "1",
      "EC2_Maximum_Hosts" : "4"
    },
    "Build": {
      "SDK_Path": "sdk-configs/Production.json",
      "sdkContentServerUrl": "https://internal.dxapi-prod.ns2p.corp.hmrc.gov.uk/services/service-name/path/"
    }
  }
}