{
  "Service": {
    "Name": "hicbc",
    "Subpath": "/hicbc/opt-in"
  },
  "Development": {
    "Environment": {
      "Type": "static-ec2",
      "Routing" : "internal",
      "Suffix": "/"
    },
    "Infrastructure": {
      "EC2_Type": "c5a.large"
    },
    "Build": {
      "SDK_Path": "sdk-configs/development.json",
      "Build_Command": "npm run build:prod:ci"
    }
  },
  "Production": {
    "Environment": {
      "Type": "hpha-ec2",
      "Routing" : "external",
      "Suffix": "/"
    },
    "Infrastructure": {
      "EC2_Type": "c5a.xlarge"
    },
    "Build": {
      "SDK_Path": "sdk-configs/development.json",
      "Build_Command": "npm run build:prod:ci"
    }
  }
}