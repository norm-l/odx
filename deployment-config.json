{
  "Service": {
    "Name": "service",
    "Subpath": "/url/suffix"
  },
  "Development": {
    "Environment": {
      "Type": "static-ec2",
      "Routing" : "internal",
      "Suffix": "/"
    },
    "Infrastructure": {
      "EC2_Type": "c5a.large",
      "EC2_Minimum_Hosts" : "1",
      "EC2_Maximum_Hosts" : "1"
    },
    "Build": {
      "SDK_Path": "sdk-configs/development.json",
      "Build_Command": "npm run build:prod:ci"
    }
  },
  "Production": {
    "Environment": {
      "Type": "hpha-ec2",
      "Routing" : "external",
      "Suffix": "/"
    },
    "Infrastructure": {
      "EC2_Type": "c5a.xlarge",
      "EC2_Minimum_Hosts" : "1",
      "EC2_Maximum_Hosts" : "4"
    },
    "Build": {
      "SDK_Path": "sdk-configs/production.json",
      "Build_Command": "npm run build:prod:ci"
    }
  }
}